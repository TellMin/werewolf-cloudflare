{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "werewolf-cloudflare",
  "main": "./dist/index.js",
  "compatibility_date": "2025-09-29",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "./dist"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "GAME_ROOM",
        "class_name": "GameRoom",
        "script_name": "werewolf-cloudflare"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["GameRoom"]
    }
  ]
